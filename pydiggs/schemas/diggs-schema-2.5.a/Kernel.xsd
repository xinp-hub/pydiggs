    <element name="Project" type="diggs:ProjectType" substitutionGroup="diggs:AbstractNamedFeature"
        abstract="false">
        <annotation>
            <documentation>A top level Diggs feature defining the business activity associated with the collection of geotechnical and geoenvironmental data.</documentation>
        </annotation>
    </element>
    <element name="ProjectEvent" type="diggs:ProjectEventType">
        <annotation>
            <documentation>An object that contains properties of time or location related events associated with a project.</documentation>
        </annotation>
    </element>
    <complexType name="ProjectEventPropertyType">
        <sequence>
            <element ref="diggs:ProjectEvent"/>
        </sequence>
    </complexType>
    <complexType name="ProjectEventType">
        <complexContent>
            <extension base="diggs:AbstractEventType">
                <sequence/>
            </extension>
        </complexContent>
    </complexType>
    <element name="ProjectGroup" substitutionGroup="diggs:AbstractGroup"
        type="diggs:ProjectGroupType">
        <annotation>
            <documentation>A top level Diggs feature defining a related group of projects.</documentation>
        </annotation>
    </element>
    <complexType name="ProjectGroupType">
        <complexContent>
            <extension base="diggs:AbstractGroupType">
                <choice>
                    <element maxOccurs="unbounded" ref="diggs:projectRef" minOccurs="1"/>
                </choice>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ProjectPropertyType">
        <sequence>
            <element ref="diggs:Project" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>A Project is a collection of sampling features, sampling
                        activities, samples, and measurements <br/>that constitutes a business
                        activity. A project may occur at a physical location, in which case it can
                        optionally contain geometries.</documentation>
                </annotation>
            </element>
        </sequence>
    </complexType>
    <element name="projectRef" type="diggs:FeatureReferenceType">
        <annotation>
            <appinfo source="urn:x-gml:targetElement">Diggs:Project</appinfo>
            <documentation>A reference to a project that uses the feature within its business
                activity. The project referenced here is the associated project for this instance.
                Other project references can be transmitted using the associatedProjectRef element.
                To identify the creating project for a feature, the property originalProjectRef
                should be used. </documentation>
            <documentation>A property element that supports a value by reference only. The target
                value is indicated in the appinfo element.</documentation>
        </annotation>
    </element>
    <complexType name="ProjectType">
        <complexContent>
            <extension base="diggs:AbstractNamedFeatureType">
                <sequence>

                    <element name="referencePoint" type="diggs:PointLocationPropertyType"
                        minOccurs="0" maxOccurs="1"/>
                    <element name="linearExtent" type="diggs:LinearExtentPropertyType" minOccurs="0"
                        maxOccurs="1"/>
                    <element name="arealExtent" type="diggs:SurfacePropertyType" minOccurs="0"
                        maxOccurs="1"/>
                    <element name="projectDateTimeSpan" type="diggs:TimeIntervalPropertyType"
                        minOccurs="0" maxOccurs="1"/>
                    <element name="locality" type="diggs:LocalityPropertyType" minOccurs="0"
                        maxOccurs="unbounded"/>
                    <element name="purpose" type="gml:CodeType" minOccurs="0" maxOccurs="1"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="projectEvent"
                        type="diggs:ProjectEventPropertyType"/>
                    <element name="contract" type="diggs:ContractPropertyType" minOccurs="0"
                        maxOccurs="unbounded"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="otherProjectProperty"
                        type="diggs:ParameterPropertyType"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element name="properties" substitutionGroup="gml:valueComponents"
        type="gml:ValueArrayPropertyType">
        <annotation>
            <documentation>A property that contains a sequence of Property objects defining the results of a test or monitoring activity.</documentation>
        </annotation>
    </element>
    <element name="Property" type="diggs:PropertyType" substitutionGroup="gml:AbstractValue"
        abstract="false">
        <annotation>
            <documentation>An object that contains properties of a measurement result.</documentation>
        </annotation>
    </element>
    <simpleType name="PropertyDataTypeEnumType">
        <union memberTypes="witsml:LogDataType diggs:dataTypeEnumType">
            <simpleType>
                <restriction base="string">
                    <enumeration value=""/>
                </restriction>
            </simpleType>
        </union>
    </simpleType>
