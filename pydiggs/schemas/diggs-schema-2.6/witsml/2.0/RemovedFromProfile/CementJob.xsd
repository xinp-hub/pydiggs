				</xs:annotation>
			</xs:element>
			<xs:element name="MixMethod" type="eml:String64" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Mix method.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NumStage" type="xs:int" minOccurs="1" maxOccurs="1">
