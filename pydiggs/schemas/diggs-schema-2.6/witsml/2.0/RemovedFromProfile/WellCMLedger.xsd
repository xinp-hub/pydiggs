			<xs:enumeration value="job"/>
			<xs:enumeration value="job plan (phases)"/>
			<xs:enumeration value="mud attributes"/>
			<xs:enumeration value="npt (lost time event)"/>
			<xs:enumeration value="time log (time measure)"/>
		</xs:restriction>
